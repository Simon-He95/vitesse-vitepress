{
  "name": "vitepress",
  "version": "0.0.0",
  "packageManager": "pnpm@6.32.3",
  "description": "vitepress",
  "author": "Simon He",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git + git@github.com:Simon-He95/vitepress.git"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/vitepress/issues"
  },
  "keywords": [
    "vitepress"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll --minify",
    "dev": "pkgroll --watch",
    "play": "vitepress dev docs --open",
    "play:build": "vitepress build docs",
    "preview": "vitepress serve docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest -u",
    "release": "bumpp --commit --tag --push && pnpm publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.1",
    "@types/node": "^17.0.31",
    "bumpp": "^7.1.1",
    "eslint": "^8.16.0",
    "pkgroll": "^1.3.1",
    "typescript": "^4.7.2",
    "vitepress": "^1.0.0-alpha.4",
    "vitest": "^0.7.0"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
