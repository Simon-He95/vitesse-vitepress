{
  "name": "vitesse",
  "version": "0.0.0",
  "packageManager": "pnpm@7.6.0",
  "description": "vitepress",
  "author": "Simon He",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git + git@github.com:Simon-He95/vitepress.git"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/vitepress/issues"
  },
  "keywords": [
    "vitepress"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.6"
  },
  "scripts": {
    "dev": "vitepress dev docs --open",
    "build": "vitepress build docs",
    "preview": "vitepress serve docs",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest -u",
    "release": "bumpp --commit --tag --push && pnpm publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.3",
    "@types/node": "^17.0.45",
    "bumpp": "^8.0.0",
    "eslint": "^8.26.0",
    "pkgroll": "^1.4.0",
    "typescript": "^4.8.4",
    "vitepress": "^1.0.0-alpha.22",
    "vitest": "^0.7.13"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
